<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <!-- Form View Inherit -->

    <record id="inherited_model_view_form_inherit_my_module" model="ir.ui.view">
        <field name="name">hr.period.view.form.inherit</field>
        <field name="model">hr.period</field>
        <field name="inherit_id" ref="hr_period.view_hr_period_form" />
        <field name="arch" type="xml">
            <field name="number" position="after">
                <field name="number_worked_days" required="True" />
                <field name="number_worked_hours" required="True" />
            </field>
        </field>
    </record>

</odoo>
