<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <template
        id="assets"
        name="pos_notification assets"
        inherit_id="point_of_sale.assets"
    >
        <xpath expr="." position="inside">
            <link rel="stylesheet" href="/pos_notification/static/src/css/pos.css" />
            <script
                type="text/javascript"
                src="/pos_notification/static/src/js/Notification.js"
            />
            <script
                type="text/javascript"
                src="/pos_notification/static/src/js/Chrome.js"
            />
            <script
                type="text/javascript"
                src="/pos_notification/static/src/js/notification_utils.js"
            />
        </xpath>
    </template>
</odoo>
