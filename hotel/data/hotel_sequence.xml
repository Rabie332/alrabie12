<odoo noupdate="1">
    <!-- Sequences for hotel folio -->
    <record id="seq_hotel_folio" model="ir.sequence">
        <field name="name">Hotel Folio</field>
        <field name="code">hotel.folio</field>
        <field name="prefix">Folio/</field>
        <field name="padding">5</field>
    </record>

    <!-- Sequences for lost -->
    <record id="sequence_hotel_lost" forcecreate="1" model="ir.sequence">
        <field name="name">Lost</field>
        <field name="code">hotel.lost.sequence</field>
        <field name="prefix">L-</field>
        <field eval="1" name="number_increment" />
        <field name="padding">5</field>
    </record>

    <!-- Sequences for found -->
    <record id="sequence_hotel_found" forcecreate="1" model="ir.sequence">
        <field name="name">Found</field>
        <field name="code">hotel.found.sequence</field>
        <field name="prefix">F-</field>
        <field eval="1" name="number_increment" />
        <field name="padding">5</field>
    </record>
</odoo>
