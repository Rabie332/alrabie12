<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <!-- Documents-->

    <record id="ir_attachment_form" model="ir.ui.view">
        <field name="name">ir.attachment.form</field>
        <field name="model">ir.attachment</field>
        <field name="inherit_id" ref="dms.ir_attachment_form" />
        <field name="priority" eval="-1" />
        <field name="arch" type="xml">
            <field name="create_date" position="attributes">
                <attribute name="readonly">1</attribute>
                <attribute name="attrs" />
            </field>
            <field name="create_uid" position="attributes">
                <attribute name="readonly">1</attribute>
                  <attribute name="attrs" />
            </field>
            <field name="write_date" position="attributes">
                <attribute name="readonly">1</attribute>
                  <attribute name="attrs" />
            </field>
        </field>
    </record>


</odoo>
