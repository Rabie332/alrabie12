            <field name="default_code">INS</field>
            <field name="purchase_ok">False</field>
            <field name="insurance">True</field>
            <field name="invoice_policy">order</field>
            <field name="taxes_id" eval="[]" />
        </record>

    <record id="product_product_returnable" model="product.product">
            <field name="name">Returnable</field>
            <field name="standard_price">0.00</field>
